<h1>Listing listings</h1>
<table>
  <tr>
    <th>Name</th>
    <th>Content</th>
    <th>Desription</th>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @listings.each do |listing| %>
  <tr>
    <td><%= listing.name %></td>
    <td><%= listing.content %></td>
    <td><%= listing.desription %></td>
    <td><%= listing.address %></td>
    <td><%= listing.city %></td>
    <td><%= listing.state %></td>
    <td><%= listing.user_id %></td>
    <td><%= listing.image %></td>
    <td><%= link_to 'Show', listing %></td>
    <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
    <td><%= link_to 'Destroy', listing, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Listing', new_listing_path %>
